<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="300.0" prefWidth="200.0"  xmlns="http://javafx.com/javafx/8.0.282"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="testClient.RegsControler">
    
    <Label layoutX="54.0" layoutY="14.0" prefHeight="38.0" prefWidth="96.0" text="Registration" textAlignment="CENTER" />


    <Label layoutX="26.0" layoutY="120.0" prefHeight="20.0" prefWidth="107.0" text="Password:" />

    <Label layoutX="26.0" layoutY="63.0" prefHeight="20.0" prefWidth="107.0" text="Nickname:" />

    
    <Label layoutX="26.0" layoutY="175.0" prefHeight="20.0" prefWidth="107.0" text="Repeat password" />


    <TextField fx:id="loginRegTF" layoutX="26.0" layoutY="83.0" prefHeight="25.0" prefWidth="150.0" />
    <PasswordField fx:id="passworRegdPF" layoutX="26.0" layoutY="138.0" prefHeight="25.0" prefWidth="150.0" />
    <PasswordField fx:id="passwordRepeatPF" layoutX="26.0" layoutY="194.0" onAction="#registration" prefHeight="25.0" prefWidth="150.0" />
    <Button fx:id="registration" layoutX="60.0" layoutY="234.0" mnemonicParsing="false" onAction="#registration"  text="Registration" />


</Pane>
